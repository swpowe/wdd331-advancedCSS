{
  "name": "parcel-demo",
  "version": "1.0.1",
  "description": "Boilerplate for a simple parcel based web app",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel serve src/index.html --dist-dir build",
    "prebuild": "rimraf dist/*",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html -d build/",
    "lint": "eslint src/**/*.js"
  },
  "author": "Shane T",
  "license": "GPL-2.0",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  }
}
